{{#extend "layout"}}
  {{#content "body"}}
      <div class="post-title">
          <h1>{{ title }}</h1>
      </div>
      <div class="post-body">
          {{ body }}
      </div>
      <div>
          {{#embed "so"}}
          {{/embed}}
      </div>
      <div>
          {{#embed "disqus"}}
          {{/embed}}
      </div>
  {{/content}}
{{/extend}}
