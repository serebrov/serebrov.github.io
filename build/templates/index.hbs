{{#extend "layout"}}
  {{#content "body"}}

    {{#each paginate.files}}
    <div class="post-preview">
      <small class="pull-right">{{formatDate this.date "%Y-%m-%d"}}</small>
      <h2><a class="title" href="{{relative ../path this.path}}">{{ this.title }}</a></h2>
      <div>{{ this.intro }}</div>
      <a class="more" href="{{relative ../path this.path}}">Read more Â»</a>
    </div>
    {{/each}}
    <div>
        {{#embed "so"}}
        {{/embed}}
    </div>

  {{/content}}
{{/extend}}
